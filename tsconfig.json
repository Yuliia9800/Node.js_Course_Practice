{
	"compilerOptions": {
		"target": "es6",
		"lib": ["es6"],
		"module": "commonJS",
		"rootDir": "src",
		// "moduleResolution": "NodeNext",
		"resolveJsonModule": true,
		"allowJs": true,
		"outDir": "build",
		"esModuleInterop": true,
		"strict": true,
		"noImplicitAny": true,
		"skipLibCheck": true
	},
	"exclude": ["./node_modules"]
}
